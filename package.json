{
  "name": "react-a11y",
  "version": "0.1.0",
  "description": "Accessibility in React applications.",
  "keywords": [
    "react",
    "reactjs",
    "accessibility",
    "a11y"
  ],
  "homepage": "https://github.com/niyowski/react-a11y#readme",
  "bugs": "https://github.com/niyowski/react-a11y/issues",
  "license": "MIT",
  "author": "Mehmet Yarar <niyowski@gmail.com>",
  "repository": "github:niyowski/react-a11y",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "clean": "pnpm dlx rimraf --glob **/node_modules",
    "reset": "pnpm clean && pnpm install",
    "format": "prettier . --write --ignore-unknown",
    "lint": "nx run-many --target=lint --all --fix",
    "typecheck": "nx run-many --target=typecheck --all",
    "test": "nx run-many --target=test --all",
    "cy": "nx run-many --target=cy --all",
    "pw": "nx run-many --target=pw --all",
    "build": "nx run-many --target=build --all",
    "demo:cy": "nx run demo:cy-watch",
    "demo:pw": "nx run demo:pw-watch",
    "demo:start": "nx run demo:serve",
    "ui:start": "nx run ui:storybook",
    "cm": "cz"
  },
  "dependencies": {
    "@swc/helpers": "~0.5.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^2.1.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/config-nx-scopes": "^18.4.3",
    "@cypress/webpack-dev-server": "^3.7.1",
    "@nx/cypress": "^17.1.3",
    "@nx/devkit": "17.1.3",
    "@nx/eslint": "17.1.3",
    "@nx/eslint-plugin": "17.1.3",
    "@nx/jest": "^17.1.3",
    "@nx/js": "17.1.3",
    "@nx/playwright": "^17.1.3",
    "@nx/react": "^17.1.3",
    "@nx/storybook": "17.1.3",
    "@nx/vite": "17.1.3",
    "@nx/web": "17.1.3",
    "@nx/workspace": "17.1.3",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/blocks": "^7.6.4",
    "@storybook/core-server": "^7.6.4",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.6.4",
    "@storybook/react-vite": "^7.6.4",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "~1.6.8",
    "@swc/cli": "~0.1.63",
    "@swc/core": "~1.3.100",
    "@swc/jest": "0.2.29",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "20.10.4",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react-swc": "~3.5.0",
    "autoprefixer": "10.4.16",
    "commitizen": "^4.3.0",
    "cypress": "^13.6.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-playwright": "^0.19.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "html-webpack-plugin": "^5.5.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "~23.0.1",
    "lint-staged": "^15.2.0",
    "nx": "17.1.3",
    "postcss": "8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "3.3.6",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.2",
    "typescript": "~5.3.3",
    "vite": "~4.5.1",
    "vite-plugin-dts": "~3.6.4",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional",
      "@commitlint/config-nx-scopes"
    ]
  }
}
